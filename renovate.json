
{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "onboarding": false,
  "requireConfig": false,
  "autodiscover": true,
  "packageManagers": [
    "pip",
    "npm",
    "yarn",
    "nuget",
    "gomod",
    "pub",
    "maven"
  ],
  "packageRules": [
    {
      "packagePatterns": [
        ".*sdl.*",
        ".*edl.*",
        ".*pdl.*"
      ],
      "matchUpdateTypes": [
        "patch",
        "minor"
      ],
      "enabled": false
    },
  ]
}
